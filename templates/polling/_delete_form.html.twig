<form method="post" action="{{ path('app_polling_delete', {'id':polling.id}) }}"
      onsubmit="return confirm('{% trans %}votings.delete.confirm{% endtrans %}');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ polling.id) }}">
    <button class="btn btn-sm btn-dark rounded-left-0">{% trans %}delete{% endtrans %}</button>
</form>
